generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id         String  @id @default(uuid())
  name       String
  details    String
  tag        String
  price      Decimal @default(0.00)
  image_path String

  @@map("products")
}
